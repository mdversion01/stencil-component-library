{
  "name": "stencil-component-library",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-component-library/stencil-component-library.esm.js",
  "type": "module",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/stencil-component-library/stencil-component-library.esm.js",
      "require": "./dist/stencil-component-library/stencil-component-library.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "my-component": [
        "dist/types/components/my-component.d.ts"
      ],
      "loader": [
        "loader/index.d.ts"
      ],
      "*": [
        "dist/types/*"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/",
    "www/build/",
    "components.d.ts",
    "readme.md",
    "LICENSE"
  ],
  "style": "dist/global.css",
  "scripts": {
    "clean": "rimraf www dist .stencil",
    "copy:mdi": "node scripts/copy-mdi.mjs",
    "postinstall": "npm run copy:mdi",
    "build": "stencil build",
    "prestart": "rimraf www/assets && rimraf www/build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec",
    "test:u": "stencil test --spec --updateSnapshot",
    "test.watch": "stencil test --spec --watchAll",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "generate": "stencil generate",
    "prestorybook": "npx stencil build",
    "storybook": "storybook dev -p 6006",
    "dev": "concurrently -n STENCIL,SB -c green,cyan \"cross-env STORYBOOK=true stencil build --dev --watch\" \"storybook dev -p 6006\"",
    "build-storybook": "storybook build",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@stencil/core": "^4.27.1",
    "@stencil/postcss": "^2.1.0",
    "@stencil/sass": "^3.2.3",
    "@storybook/addon-a11y": "8.6.15",
    "@storybook/addon-docs": "8.6.15",
    "@storybook/addon-essentials": "8.6.15",
    "@storybook/addon-interactions": "8.6.15",
    "@storybook/test": "8.6.15",
    "@storybook/web-components-vite": "^8.6.15",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.24.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^7.0.2",
    "puppeteer": "^24.3.0",
    "rimraf": "^6.0.1",
    "sass": "^1.97.0",
    "storybook": "^8.6.15",
    "ts-jest": "^29.3.4",
    "typescript-eslint": "^8.30.1",
    "vite": "^5.4.21"
  },
  "license": "MIT",
  "dependencies": {
    "@floating-ui/dom": "^1.7.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@mdi/font": "^7.4.47",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.5",
    "stenciljs-bootstrap": "^0.0.34"
  }
}
